;*** Patch the needed files...

echo " ****** Patching files..."

spatch -pNCUBE.pch -oram:temp_out LOGIC:NCUBE.EXE
copy >NIL: ram:temp_out LOGIC:NCUBE.EXE
delete >NIL: ram:temp_out

