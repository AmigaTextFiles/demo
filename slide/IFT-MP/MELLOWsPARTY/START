echo "INFECT RELEASE" 
echo " "
echo "SLIDE SHOW IS COMING UP NEXT"
ECHO " "
ECHO " LEAN BACK...  YOU HAVE NOTHING TO CLICK... "
ECHO " RELEASEDATE : 24.09.1996"
